{
	"compileOnSave": false,
	"compilerOptions": {
		"outDir": "./dist/out-tsc",
		"baseUrl": "src",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noUnusedLocals": true,
        "noUnusedParameters": false,
		"target": "es6",
		"typeRoots": [
			"node_modules/@types"
		],
		"paths": {
			"robot/*": ["app/robot/*"],
			"wifi/*": ["app/wifi/*"] 
		},
		"lib": [
			"es2016",
			"dom"
		]
	}
}